    "\n",
    "D = A-B\n",
    "print(\"A - B=\\n\",D)\n",
    "\n",
    "E = np.dot(A, B)\n",
    "print(\"A * B (dot product) =\\n\", E)\n",
    "\n",
    "print(\"Transpose of A:\\n\", A.T)\n",
